
while True:
    enterPass = input("Enter Password: ")
    letter = False
    number = False

    for char in enterPass:
        if char.isalpha():
            letter = True
        elif char.isdigit():
            number = True

    if not letter:
        print("Engk! Must contain letter")
    elif not number:
        print("Engk! Must contain number")
    else: 
        print("Yey! Password Accepted")
        break 

    
